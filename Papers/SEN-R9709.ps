	cwiscale dup scale
	58 153 t			% Make the lowerleft corner (0,0)
	S
	220 125 t
	    0 0 RR 40 add 0 360 arc 1 setgray fill
	R
	S
	    1.02 dup scale
	    caption			% CWI
	R
	220 125 t
	/lw 6 def
	outside
    R
} def
