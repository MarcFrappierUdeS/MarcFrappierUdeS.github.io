%%BeginProcSet: fmt.ps
/fmt_dict 16 dict def
fmt_dict begin
/SP 32 def
/TAB 9 def
/NL 10 def
/CR 13 def
/LF {
    %
    % Check on a linefeed symbol.
    % integer LF --> integer boolean
    %
    dup dup NL eq exch CR eq or
} def
/WS {
    %
    % Check on a whitespace.
    % integer WS --> integer boolean
    %
    dup LF exch dup SP eq exch TAB eq or or
} def

/fmt {
    %
    % string linelength fmt --> #_of_lines_emitted
    %
    % Show "string" while wrapping at linelength
    % This procedure will only work reasonable well
    % when the tokens in string are small since there
    % is no look-ahead...
    %
    /ll exch def
    /nlines 1 def
    0 0 moveto {
	pop WS {
	    currentpoint pop ll gt {
		0 VS neg translate
		0 0 moveto
		/nlines nlines 1 add def
	    }{
		LF { ( ) show } if
	    } ifelse
	} if
	pop
    } exch kshow
    nlines
} def
end
